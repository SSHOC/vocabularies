@prefix bibo:  <http://purl.org/ontology/bibo/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

bibo:Journal a skos:Concept ;
          skos:inScheme bibo:publicationTypeVocabulary ;
          skos:topConceptOf bibo:publicationTypeVocabulary ;
          rdfs:label "Journal"@en ;
          skos:prefLabel "Journal"@en .

bibo:Book a skos:Concept ;
          skos:inScheme bibo:publicationTypeVocabulary ;
          skos:topConceptOf bibo:publicationTypeVocabulary ;
          rdfs:label "Book"@en ;
          skos:prefLabel "Book"@en .

bibo:Conference a skos:Concept ;
          skos:inScheme bibo:publicationTypeVocabulary ;
          skos:topConceptOf bibo:publicationTypeVocabulary ;
          rdfs:label "Conference"@en ;
          skos:prefLabel "Conference"@en .

bibo:Article a skos:Concept ;
          skos:inScheme bibo:publicationTypeVocabulary ;
          skos:topConceptOf bibo:publicationTypeVocabulary ;
          rdfs:label "Article"@en ;
          skos:prefLabel "Article"@en .

bibo:Pre-Print a skos:Concept ;
          skos:inScheme bibo:publicationTypeVocabulary ;
          skos:topConceptOf bibo:publicationTypeVocabulary ;
          rdfs:label "Pre-Print"@en ;
          skos:prefLabel "Pre-Print"@en .

bibo:publicationTypeVocabulary a skos:ConceptScheme ;
          rdfs:label "The Bibliographic Ontology Concept Scheme"@en ;
          dc:title "The Bibliographic Ontology Concept Scheme"@en ;
          dct:title "The Bibliographic Ontology Concept Scheme"@en ;
          rdfs:comment "The Bibliographic Ontology describes bibliographic things on the semantic Web in RDF. This ontology can be used as a citation ontology, as a document classification ontology, or simply as a way to describe any kind of document in RDF. It has been inspired by many existing document description metadata formats, and can be used as a common ground for converting other bibliographic data sources."@en ;
          dc:description "The Bibliographic Ontology describes bibliographic things on the semantic Web in RDF. This ontology can be used as a citation ontology, as a document classification ontology, or simply as a way to describe any kind of document in RDF. It has been inspired by many existing document description metadata formats, and can be used as a common ground for converting other bibliographic data sources."@en ;
          skos:hasTopConcept bibo:Journal ;
          skos:hasTopConcept bibo:Book ;
          skos:hasTopConcept bibo:Conference ;
          skos:hasTopConcept bibo:Article ;
          skos:hasTopConcept bibo:Pre-Print .
